<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>RegCalReporting</display-name>
    <distributable/>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.seta.servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperazioniEnte</servlet-name>
        <servlet-class>com.seta.servlet.OperazioniEnte</servlet-class>
        <multipart-config>
            <max-file-size>10485760</max-file-size>
            <max-request-size>20971520</max-request-size>
            <file-size-threshold>5242880</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>OperazioniGeneral</servlet-name>
        <servlet-class>com.seta.servlet.OperazioniGeneral</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperazioniRegional</servlet-name>
        <servlet-class>com.seta.servlet.OperazioniRegional</servlet-class>
        <multipart-config>
            <max-file-size>10485760</max-file-size>
            <max-request-size>20971520</max-request-size>
            <file-size-threshold>5242880</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>QueryEnte</servlet-name>
        <servlet-class>com.seta.servlet.QueryEnte</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryRegional</servlet-name>
        <servlet-class>com.seta.servlet.QueryRegional</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryRevisore</servlet-name>
        <servlet-class>com.seta.servlet.QueryRevisore</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperazioniRevisore</servlet-name>
        <servlet-class>com.seta.servlet.OperazioniRevisore</servlet-class>
        <multipart-config>
            <max-file-size>10485760</max-file-size>
            <max-request-size>20971520</max-request-size>
            <file-size-threshold>5242880</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperazioniEnte</servlet-name>
        <url-pattern>/OperazioniEnte</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperazioniGeneral</servlet-name>
        <url-pattern>/OperazioniGeneral</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperazioniRegional</servlet-name>
        <url-pattern>/OperazioniRegional</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryEnte</servlet-name>
        <url-pattern>/QueryEnte</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryRegional</servlet-name>
        <url-pattern>/QueryRegional</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryRevisore</servlet-name>
        <url-pattern>/QueryRevisore</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperazioniRevisore</servlet-name>
        <url-pattern>/OperazioniRevisore</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>404</error-code>
        <location>/page_404.html</location>
    </error-page>
    <error-page>
        <error-code>400</error-code>
        <location>/page_400.html</location>
    </error-page>
    <error-page>
        <error-code>403</error-code>
        <location>/page_403.html</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/page_500.html</location>
    </error-page>
</web-app>
